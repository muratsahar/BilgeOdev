@model List<BilgeadamHomework.Models.EntityFramework.Personel>
<h1>Personel Lİstesi</h1>
<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Ad</th>
                <th>Soyad</th>
                <th>Doğum Tarihi</th>
                <th>Maaş</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var personel in Model)
            {
                <tr>
                    <td>@personel.Ad</td>
                    <td>@personel.Soyad</td>
                    <td>@personel.DogumTarihi.Value.ToShortDateString()</td>
                    <td>@personel.Maas TL</td>
                </tr>
            }

        </tbody>
    </table>
</div>
